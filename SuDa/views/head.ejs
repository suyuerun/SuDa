		<script src="/javascripts/jquery-2.1.4.min.js"></script>
		<script src="http://cdn.static.runoob.com/libs/bootstrap/3.3.7/js/bootstrap.min.js"></script>

		<style>
			.p_logo{
				margin: left:100px;
			}
			.headStyle {
				height: 2.4em;
				margin-top: 6px;
			}
			.rowbg{
				background-image: url(../images/bg/headbg.png);
				
			}
			.headmove {
				width: 100%;
				height: 100%;
				position: relative;
				animation: myheadmove 1s ;
				animation-iteration-count:1 ;
				-moz-animation: myheadmove 1s ;
				/*Firefox*/
				-webkit-animation: myheadmove 1s ;
				/*Safari and Chrome*/
			}
			
			@keyframes myheadmove {
				from {
					top: -300px;
				}
				to {
					top: 0px;
				}
			}
			
			@-moz-keyframes myheadmove
			/*Firefox*/
			
			{
					from {
					top: -300px;
				}
				to {
					top: 0px;
				}
			}
			
			@-webkit-keyframes myheadmove
			/*Safari and Chrome*/
			
			{
					from {
					top: -300px;
				}
				to {
					top: 0px;
				}
			}
		</style>
		
	</head>

